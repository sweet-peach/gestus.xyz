#Profile can be "development" or "production"
PROFILE=development

### GENERAL SETTINGS
# Specifies the network address to which the server is bind at startup. Default value is 'localhost'
# LISTEN_ADDRESS = ''

#PostgreSQL data for connect
DATABASE_NAME=test
DATABASE_USERNAME=user
DATABASE_PASSWORD=pass
DATABASE_IP=localhost
DATABASE_PORT=5432

#Secret key in BASE64
JWT_
#Expiration date in miliseconds
JWT_EXPIRATION=3600000

#When the server boots it will try to create root user with credentials below
ROOT_USER_EMAIL=admin@example.com
ROOT_USER_PASSWORD=admin

#Your server storage limit in bytes
STORAGE_LIMIT=1000000000

# Elasticsearch 8.x.x connect configuration
ELASTIC_URL=http://localhost:9200
ELASTIC_USERNAME=user
ELASTIC_PASSWORD=pass

###Development settings
CORS_ALLOWED_ORIGIN=http://localhost:5173/